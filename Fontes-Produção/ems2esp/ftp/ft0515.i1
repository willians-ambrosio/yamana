              if nota-fiscal.nat-operacao <> "6902ii" and nota-fiscal.nat-operacao <> "7101ii" then
                 put i-qt-volumes[i]                                    at 6.
              
              put /* i-qt-volumes[i]                                    at 6  inibido por Patricia em 15/01 */
                  c-especie[i]                                          at 32.
           end.       
           else put " " at 32.
        end.

   end.

