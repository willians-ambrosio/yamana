*/
    end.

/*--------------IMPRESSAO DA DESCRICAO DO ITEM ----------------------------*/
/*
    find first tt-editor no-lock no-error.
    if avail tt-editor then delete tt-editor.
*/

  if item.tipo-contr = 4 then do:
    IF item.ind-imp-desc = 2 OR
       item.ind-imp-desc = 4 OR 
       item.ind-imp-desc = 5 OR
       item.ind-imp-desc = 6 OR
       item.ind-imp-desc = 7 THEN DO:             

      FOR EACH tt-editor:
        assign i-cont-item = i-cont-item + 1.
          put tt-editor.conteudo format "x(50)" at 8.

        IF (i-cont-item / 12) - INT(i-cont-item / 12) = 0 THEN DO:
          PUT SKIP(2).
          {ftp\ft0515.i3}
          PUT SKIP(7).  
          PUT SKIP(7).
          PUT SKIP(7).
          PUT c-num-nota AT 130 SKIP(6).
          {ftp\ft0515.i2}
        END.
      end.
    end.
  end.

/*----------------------------  ACUMULA TOTAIS  ------------------------------*/

    assign de-tot-bas-icm   = de-tot-bas-icm   + (if it-nota-fisc.cd-trib-icm = 3 then it-nota-fisc.vl-icmsou-it else it-nota-fisc.vl-bicms-it)
           de-tot-icm       = de-tot-icm       + (if it-nota-fisc.cd-trib-icm <> 2 AND
                                                     it-nota-fisc.cd-trib-icm <> 5
                                                  then it-nota-fisc.vl-icms-it
                                                  else 0)
           de-tot-bas-ipi   = de-tot-bas-ipi   + (if it-nota-fisc.cd-trib-ipi = 3 then it-nota-fisc.vl-ipiou-it else it-nota-fisc.vl-bipi-it)
           de-tot-ipi       = de-tot-ipi       + (if it-nota-fisc.cd-trib-ipi <> 2
                                                  then it-nota-fisc.vl-ipi-it
                                                  else 0)
           de-tot-bas-iss   = de-tot-bas-iss   + it-nota-fisc.vl-biss-it
           de-tot-iss       = de-tot-iss       + it-nota-fisc.vl-iss-it
           de-tot-icmssubs  = de-tot-icmssubs  + it-nota-fisc.vl-icmsub-it
           de-tot-bicmssubs = de-tot-bicmssubs + it-nota-fisc.vl-bsubs-it.

   IF natur-oper.cd-trib-icm  = 5 THEN do: /**** DIFERIDO ****/
     CASE it-nota-fisc.cd-trib-icm:
       WHEN 5 THEN ASSIGN de-tot-bas-icm = de-tot-bas-icm + it-nota-fisc.vl-icmsou-it.
       WHEN 3 THEN ASSIGN de-tot-bas-icm = de-tot-bas-icm + it-nota-fisc.vl-icmsou-it.
       WHEN 2 THEN ASSIGN de-tot-bas-icm = de-tot-bas-icm + it-nota-fisc.vl-icmsnt-it.
       WHEN 1 THEN ASSIGN de-tot-bas-icm = de-tot-bas-icm + it-nota-fisc.vl-icmsou-it.  
     END CASE.
   END.      
   
/* ---> BASE DE CALCULO - OUTROS / IMPORTA€ÇO <--- */
   if natur-oper.tipo          = 1 and /*** Nota Fiscal de Entrada ***/
      natur-oper.mercado       = 2 and /*** Mercado Externo ***/ 
      natur-oper.cd-trib-icm   = 3  then do:
      ASSIGN de-tot-bas-icm   = de-tot-bas-icm   +  it-nota-fisc.vl-icmsou-it.
   end.

/* ---> ICMS OUTROS - Tratamento para quando o c¢d. Trib. do ICMS for OUTROS, o valor de Despesa Acess¢ria impresso no Formul rio:
          VL DESPESA ACESSàRIA = VL DESPESA ACESSàRIA - VALOR ICMS OUTROS. 
          Wellington Ap - DSC - 18/09/08 <--- */  

    /* ---> DESPESA ACESSORIO <--- */
   if natur-oper.tipo          = 1 and /*** Nota Fiscal de Entrada ***/
      natur-oper.mercado       = 2 /*** Mercado Externo ***/ then
      assign de-vl-embalagem = de-vl-embalagem - it-nota-fisc.vl-icms-it.
        
/* ---------------------> FIM TRATAMENTO ICMS <------------------------ */

    assign i-cont-item = i-cont-item + 1. /* controle de linhas */

        IF i-cont-item >= 11 THEN DO:
           if i-cont-item = 11 then put skip(1).
           
           assign i-cont-item = 0.
          
           PUT SKIP(2).
           {ftp\ft0515.i3}
           PUT SKIP(7).  
           PUT SKIP(7).
           PUT SKIP(7).
           PUT c-num-nota AT 130 SKIP(6).
           {ftp\ft0515.i2}
        END.
/* ---------------------> FIM TRATAMENTO PULO PAG <------------------------ */
  end.

/* ---> WEllington Ap - DSC - 18/09/08 
     LOGICA PARA IMPRIMIR *** NOS CAMPOS DE VALORES.<--- */
  IF l-mais-1-pag THEN DO:
    IF i-cont-item < 12 THEN
      PUT SKIP (14 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 12 AND i-cont-item <= 24 THEN
      PUT SKIP (26 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 25 AND i-cont-item <= 36 THEN
      PUT SKIP (38 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 37 AND i-cont-item <= 48 THEN
      PUT SKIP (50 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 49 AND i-cont-item <= 60 THEN
      PUT SKIP (62 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 61 AND i-cont-item <= 72 THEN
      PUT SKIP (74 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 73 AND i-cont-item <= 84 THEN
      PUT SKIP (86 - i-cont-item).  /* nr de linhas a serem puladas */
      
    IF i-cont-item >= 85 AND i-cont-item <= 96 THEN
      PUT SKIP (98 - i-cont-item).  /* nr de linhas a serem puladas */
  END.
  ELSE
    PUT SKIP (14 - i-cont-item).  /* nr de linhas a serem puladas */

/*---------------------------  IMPRIME TOTAIS  -------------------------------*/
  if  de-tot-bas-iss <> 0 then do:
      assign de-aliquota-iss = de-tot-iss * 100 / de-tot-bas-iss.
  end.
  else do:
      assign de-aliquota-iss = 0 .
  end.
 
  find natur-oper no-lock
      where natur-oper.nat-operacao = nota-fiscal.nat-operacao no-error.

  find first emitente-cex no-lock 
       where emitente-cex.cod-emitente = emitente.cod-emitente no-error.
       
  if avail emitente-cex and 
           natur-oper.mercado = 2  then 
     assign d-embalagem-icm = nota-fiscal.vl-embalagem - de-tot-icm.
  else
     assign d-embalagem-icm = nota-fiscal.vl-embalagem.

  if de-tot-icmssubs-obs <> 0 then
     if estabelec.estado = "SP" and
        emitente.estado  = "SP" then
              assign de-tot-icm = 0. 


  put   de-tot-bas-icm           format ">>>,>>>,>>9.99"  at 14
        de-tot-icm               format ">>>,>>>,>>9.99"  at 41
        de-tot-bicmssubs         format ">>>,>>>,>>9.99"  at 67
        de-tot-icmssubs          format ">>>,>>>,>>9.99"  at 96
        nota-fiscal.vl-mercad    format ">>>,>>>,>>9.99"  at 130 skip (1).

  put   nota-fiscal.vl-frete     format ">>>,>>>,>>9.99"  at 14
