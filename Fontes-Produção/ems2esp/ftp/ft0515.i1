   end.
   else do:

      /* Incluido por Helder, solicitado por Ricardo Pinto 18/01/2006 */
      if nota-fiscal.nat-operacao <>"6902ii" and nota-fiscal.nat-operacao <> "7101ii" then
         put i-qt-volumes[1]                                       at  6.

      put /* i-qt-volumes[1]                                       at  6   inibido por Patricia em 15/01 */
           c-especie[1]                                            at 32
           nota-fiscal.marca-volume                                at 62.
           /*nota-fiscal.nr-volumes                                at 90
             nota-fiscal.peso-bru-tot   format ">>>,>>>,>>9.999"   at 107
             nota-fiscal.peso-liq-tot   format ">>>,>>>,>>9.999"   at 128 inibido por Patricia em 15/01*/
             
      /* Incluido por Helder, solicitado por Ricardo Pinto 18/01/2006 */
      if nota-fiscal.nat-operacao <> "6902ii" and nota-fiscal.nat-operacao <> "7101ii" then
         put nota-fiscal.nr-volumes                                at 90
             nota-fiscal.peso-bru-tot   format ">>>,>>>,>>9.999"   at 107
             nota-fiscal.peso-liq-tot   format ">>>,>>>,>>9.999"   at 128.

        do i = 2 to 5:
           if i-qt-volumes[i] <> 0 then
           do:
