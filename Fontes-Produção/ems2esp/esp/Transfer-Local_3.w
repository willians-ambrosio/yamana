ON CHOOSE OF bt-arquivo-entrada IN FRAME f-pg-par
DO:
/*     {include/i-imarq.i c-arquivo-entrada f-pg-par} */

    /*---------------------------------------------------------------*/
    def var c-arq-conv  as char no-undo.

    assign c-arq-conv = replace(input frame f-pg-par c-arquivo-entrada, "/", "\").

    SYSTEM-DIALOG GET-FILE c-arq-conv
       FILTERS &IF "{3}" <> "" &THEN {3} 
               &ENDIF
               &IF "{3}" = "" &THEN
               "*.xlsx" "*.xlsx",
               "*.*" "*.*"         
               &ENDIF      
/*        &IF '{1}' <> 'c-arquivo-entrada' &THEN */
/*            ASK-OVERWRITE                      */
/*            SAVE-AS                            */
/*        &ENDIF                                 */
       DEFAULT-EXTENSION "xlsx"
       INITIAL-DIR "spool" 
       USE-FILENAME
       UPDATE l-ok.
    if  l-ok = yes then do:
        assign c-arquivo-entrada = replace(c-arq-conv, "\", "/").
        display c-arquivo-entrada with frame f-pg-par.
    end.
    /*---------------------------------------------------------------*/
END.

