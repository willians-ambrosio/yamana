  IF c-arquivo-entrada = "" THEN
    MESSAGE "Nenhum Arquivo foi Informado!"
        VIEW-AS ALERT-BOX ERROR BUTTONS OK.
  ELSE
  IF SEARCH(c-arquivo-entrada) = ? THEN
    MESSAGE "Arquivo " c-arquivo-entrada " nao foi encontrado!"
        VIEW-AS ALERT-BOX ERROR BUTTONS OK.
  ELSE
  IF SEARCH(c-arquivo-entrada) <> ? THEN
    DO:
      RUN esp\Transfer-Local_4.p(INPUT c-arquivo-entrada).
    END.
END.

